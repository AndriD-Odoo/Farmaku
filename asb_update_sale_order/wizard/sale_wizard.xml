<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sale_wizard_view_form" model="ir.ui.view">
        <field name="name">sale.wizard.view.form</field>
        <field name="model">sale.wizard</field>
        <field name="arch" type="xml">
            <form string="Update">
                <sheet>
                    <group>
                        <field name="name" invisible="1"/>
                        <field name="upload_file" filename="name" widget="binary"/>
                    </group>
                    <footer>
                        <button name="button_bulk_create" type="object" string="Update" class="oe_highlight"/>
                        <button special="cancel" string="Batal"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>